{
  "name": "cms-react-starter-theme",
  "version": "1.0.0",
  "description": "CMS React starter theme",
  "scripts": {
    "start": "hs-cms-dev-server .",
    "deploy": "hs project upload",
    "build:css": "tailwindcss -c ./tailwind.config.js -i ./styles/tailwind.css -o ./assets/css/tailwind.css --minify",
    "dev:css": "tailwindcss -c ./tailwind.config.js -i ./styles/tailwind.css -o ./assets/css/tailwind.css --watch",
    "dev": "concurrently \"npm run watch:css\" \"npm run start\"",
    "build": "npm run build:css",
    "build:deploy": "npm run build:css && hs project upload"
  },
  "type": "module",
  "keywords": [],
  "dependencies": {
    "@headlessui/react": "2.2.7",
    "@heroicons/react": "2.1.5",
    "@hubspot/cms-components": "latest",
    "@tailwindcss/postcss": "^4.1.12",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwindcss": "^4.1.12"
  },
  "devDependencies": {
    "@hubspot/cms-dev-server": "latest"
  },
  "overrides": {
    "react-dom": "18.3.1"
  }
}
